<!ELEMENT store (store_details?,heading?,songs?,footer?)>


<!ELEMENT store_details (store_name?,logo?,quote?,background?,description?)>


<!ELEMENT store_name (#PCDATA)>


<!ELEMENT logo EMPTY>


